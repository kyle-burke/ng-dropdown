{
  "compilerOptions": {
    "target": "es5",
    "module": "es2015",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [ "es2015", "dom" ],
    "noImplicitAny": true,
    "strictNullChecks": true,
    "stripInternal": true,
    "skipLibCheck": true,
    "paths": {
      "@angular/core": ["node_modules/@angular/core"]
    },
    "angularCompilerOptions": {
      "strictMetadataEmit": true
    },
    "declaration": true
  },
  "exclude": ["node_modules"]
}
